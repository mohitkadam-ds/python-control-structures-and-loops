# Demonstration of break, continue and pass
print("Using break:")
for i in range(1, 11):
    if i == 5:
        break
    print(i)
print("\nUsing continue:")
for i in range(1, 11):
    if i == 5:
        continue
    print(i)
print("\nUsing pass:")
for i in range(1, 6):
    if i == 3:
        pass
    else:
        print(i)
